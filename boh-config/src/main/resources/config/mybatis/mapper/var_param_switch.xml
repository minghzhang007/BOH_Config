<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.lewis.bohconfig.dao.IVarParamSwitchDao">
    <resultMap id="varParamSwitch" type="com.lewis.bohconfig.domain.VarParamSwitch">
        <result property="id" column="id"/>
        <result property="identity" column="identity"/>
        <result property="description" column="description"/>
        <result property="content" column="content"/>
        <result property="delFlag" column="del_flag"/>
    </resultMap>
    
    <sql id="varParamSwitchColumns">
        id,identity,description,content,del_flag
    </sql>

    <select id="getAllVarParamSwitch" resultMap="varParamSwitch">
        SELECT
        <include refid="varParamSwitchColumns"/>
        FROM var_param_switch
        WHERE del_flag = 0
    </select>

    
</mapper>